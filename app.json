{
  "expo": {
    "name": "BuddyBoard",
    "slug": "buddyboard-mobile",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "ios": {
      "bundleIdentifier": "com.azlabs.buddyboard",
      "buildNumber": "1.0.4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "orientation": "portrait",
    "plugins": [
      "expo-asset",
      "expo-notifications"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.azlabs.buddyboard",
      "versionCode": 2,
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "38ed77a1-3892-42c8-b7cf-9cebe6d98545"
      }
    },
    "owner": "hligon"
  }
}
