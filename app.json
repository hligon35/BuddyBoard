{
  "expo": {
    "name": "BuddyBoard",
    "slug": "buddyboard-mobile",
    "version": "1.0.0",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "orientation": "portrait",
    "plugins": [
      "expo-asset"
    ]
    ,
    "icon": "./assets/BuddyBoardicon.png",
    "splash": {
      "image": "./assets/buddyBoardSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/BuddyBoardicon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/BuddyBoardicon.png"
    }
  }
}
