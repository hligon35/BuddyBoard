{
  "name": "buddyboard-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android:internal": "eas build -p android --profile internal --non-interactive",
    "build:android:internal:interactive": "eas build -p android --profile internal",
    "submit:android:internal": "eas submit -p android --latest --non-interactive --key ./google-play-service-account.json",
    "dev-clear-server": "node ./scripts/dev-clear-server.js",
    "api:server": "node ./scripts/api-server.js",
    "api:mock": "set PORT=3006 && node ./scripts/api-mock.js",
    "smoke": "node ./scripts/smoke-e2e.js",
    "smoke:mock": "set BB_BASE_URL=http://127.0.0.1:3006 && node ./scripts/smoke-e2e.js",
    "smoke:server": "set BB_BASE_URL=http://127.0.0.1:3005 && node ./scripts/smoke-e2e.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "axios": "^1.13.2",
    "babel-preset-expo": "~54.0.9",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^11.10.0",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "expo": "~54.0.30",
    "expo-asset": "~12.0.12",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-image-picker": "~17.0.10",
    "expo-local-authentication": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.15",
    "expo-secure-store": "^15.0.8",
    "express": "^5.2.1",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "nativewind": "^4.2.1",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^4.1.17",
    "twilio": "^5.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@react-native-community/cli": "latest",
    "@react-native-community/cli-server-api": "^20.0.2",
    "expo-module-scripts": "^5.0.8"
  },
  "overrides": {
    "react-native-renderer": "19.1.0",
    "react": "19.1.0"
  }
}
